TestType<A, B, C, D, SeventyTwo>

Option<T> :: [Some T | None]

unwrap<T> :: Option<T> -> T
unwrap Some = $
unwrap None = panic "Unwrapped None variant of Option"

Vec2<T> :: (T, T)

add<T : Add<T, T, T>> :: (Vec2<T>, Vec2<T>) -> Vec2<T>
add = flatten $
    -> apply $ (apply $ first third -> add $) (apply $ second fourth -> add $)
